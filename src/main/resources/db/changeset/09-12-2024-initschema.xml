<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <changeSet id="09-12-2024-initschema.xml" author="rog">
        <createTable tableName="persons" remarks="table of persons">
            <column name="id" type="integer" autoIncrement="true" startWith="1" incrementBy="1">
                <constraints primaryKey="true"/>
            </column>
            <column name="name" type="text">
                <constraints nullable="false"/>
            </column>
            <column name="birth_date" type="timestamp with time zone"/>
            <column name="age" type="integer"/>
        </createTable>
    </changeSet>
</databaseChangeLog>